{
    "name": "dash-web",
    "version": "0.1.0",
    "private": true,
    "engines": {
        "node": "15.8.0",
        "npm": "7.5.2"
    },
    "scripts": {
        "start": "set HTTPS=true && react-scripts -r @cypress/instrument-cra start",
        "build": "react-scripts build --prod",
        "server": "node src/server.js",
        "test": "react-scripts test --env=jsdom",
        "coverage": "npm run test -- --coverage --watchAll=false",
        "ci": "yarn run coverage && run-p -r start cy:run",
        "cy:open": "cypress open",
        "cy:run": "wait-on http://localhost:3000 && cypress run --headless --browser chrome --record --key 344b3965-0e11-4597-9ee6-aa087d1ab63e",
        "cy:verify": "cypress verify",
        "lint:fix": "eslint . --ext .js,.ts,.tsx --fix",
        "lint:styles": "stylelint \"src/**/*.{css,scss,sass}\" --formatter json --output-file ./styleLintReport.json",
        "report:merge": "mochawesome-merge ./cypress/results/mochawesome-report/*.json > tests-report.json",
        "report:generate": "marge tests-report.json -o cypress/reports/html",
        "report:copyScreenshots": "cp -r cypress/screenshots cypress/reports/html/screenshots"
    },
    "browserslist": [
        ">0.2%",
        "not ie <= 11",
        "not op_mini all"
    ],
    "dependencies": {
        "@toast-ui/react-calendar": "^1.0.5",
        "axios": "^0.21.1",
        "bootstrap": "^4.5.0",
        "chart.js": "^2.9.3",
        "dayjs": "^1.8.29",
        "express": "4.17.1",
        "font-awesome": "4.7.0",
        "ical": "^0.8.0",
        "pg": "^8.5.1",
        "query-string": "^6.13.1",
        "react": "^16.14.0",
        "react-beautiful-dnd": "^13.0.0",
        "react-chartjs-2": "^2.9.0",
        "react-dom": "^16.9.12",
        "react-redux": "^7.2.0",
        "react-router-dom": "5.2.0",
        "react-scripts": "^4.0.3",
        "reactstrap": "^8.5.1",
        "rss-parser": "^3.9.0",
        "winston": "^3.3.3"
    },
    "devDependencies": {
        "@cypress/code-coverage": "^3.9.3",
        "@cypress/instrument-cra": "^1.4.0",
        "@types/chart.js": "^2.9.22",
        "@types/enzyme": "^3.10.5",
        "@types/enzyme-adapter-react-16": "1.0.6",
        "@types/express": "^4.17.6",
        "@types/fb": "^0.0.28",
        "@types/ical": "^0.6.1",
        "@types/jest": "^26.0.20",
        "@types/node": "^14.14.25",
        "@types/pg": "^7.14.4",
        "@types/react": "^16.14.0",
        "@types/react-beautiful-dnd": "^13.0.0",
        "@types/react-dom": "^16.9.12",
        "@types/react-redux": "^7.1.9",
        "@types/react-router-dom": "^5.1.5",
        "@types/reactstrap": "^8.5.0",
        "@types/xml2js": "^0.4.5",
        "@typescript-eslint/eslint-plugin": "^4.20.0",
        "@typescript-eslint/parser": "^4.20.0",
        "cypress": "^6.8.0",
        "cypress-multi-reporters": "^1.4.0",
        "cypress-wait-until": "^1.7.1",
        "enzyme": "^3.11.0",
        "enzyme-adapter-react-16": "^1.15.6",
        "eslint": "^7.23.0",
        "eslint-config-prettier": "^8.1.0",
        "istanbul-lib-coverage": "^3.0.0",
        "mochawesome": "^6.2.2",
        "mochawesome-merge": "^4.2.0",
        "mochawesome-report-generator": "^5.2.0",
        "node-sass": "npm:sass@^1.32.6",
        "nyc": "^15.1.0",
        "stylelint": "^13.12.0",
        "stylelint-config-standard": "^21.0.0",
        "ts-jest": "22.4.6",
        "tslib": "^2.1.0",
        "tslint-react-hooks": "^2.2.2",
        "typescript": "^4.2.3",
        "wait-on": "^5.3.0"
    }
}