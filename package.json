{
    "name": "dash-web",
    "version": "0.1.0",
    "private": true,
    "engines": {
        "node": "13.8.0",
        "npm": "6.13.6"
    },
    "scripts": {
        "start": "set HTTPS=true && react-scripts-ts start",
        "build": "react-scripts-ts build --prod",
        "preinstall": "npm i -g typescript",
        "server": "node src/server.js",
        "test": "react-scripts-ts test --env=jsdom",
        "coverage": "react-scripts-ts test --env=jsdom --coverage",
        "ci": "npm test -- --coverage && run-p -r start cy:run",
        "cy:open": "cypress open",
        "cy:run": "wait-on http://localhost:3000 && cypress run",
        "cy:run:record": "cypress run --record"
    },
    "browserslist": [
        ">0.2%",
        "not ie <= 11",
        "not op_mini all"
    ],
    "dependencies": {
        "@toast-ui/react-calendar": "^1.0.5",
        "axios": "0.19.2",
        "bootstrap": "^4.5.0",
        "chart.js": "^2.9.3",
        "dayjs": "^1.8.29",
        "express": "4.17.1",
        "font-awesome": "4.7.0",
        "ical": "^0.8.0",
        "pg": "8.3.0",
        "query-string": "^6.13.1",
        "react": "16.13.1",
        "react-chartjs-2": "^2.9.0",
        "react-dom": "16.13.1",
        "react-redux": "^7.2.0",
        "react-router-dom": "5.2.0",
        "react-scripts-ts": "^4.0.8",
        "reactstrap": "^8.5.1",
        "rss-parser": "^3.9.0",
        "winston": "^3.3.3"
    },
    "devDependencies": {
        "@types/chart.js": "^2.9.22",
        "@types/enzyme": "^3.10.5",
        "@types/enzyme-adapter-react-16": "1.0.6",
        "@types/express": "^4.17.6",
        "@types/fb": "^0.0.28",
        "@types/ical": "^0.6.1",
        "@types/jest": "25.1.4",
        "@types/node": "14.0.22",
        "@types/pg": "^7.14.4",
        "@types/react": "16.9.42",
        "@types/react-dom": "16.9.7",
        "@types/react-redux": "^7.1.9",
        "@types/react-router-dom": "^5.1.5",
        "@types/reactstrap": "^8.5.0",
        "@types/xml2js": "^0.4.5",
        "cypress": "4.3.0",
        "enzyme": "^3.11.0",
        "enzyme-adapter-react-16": "1.15.2",
        "node-sass": "^4.14.1",
        "ts-jest": "^22.4.6",
        "tslint-react-hooks": "^2.2.2",
        "typescript": "3.9.6"
    }
}
