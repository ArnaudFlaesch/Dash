{
  "name": "dash-web",
  "version": "0.2.0",
  "private": true,
  "engines": {
    "node": ">=14.0.0 <=15.12.0",
    "npm": "7.11.1"
  },
  "scripts": {
    "start": "react-scripts -r @cypress/instrument-cra start",
    "build": "react-scripts build --prod",
    "server": "node src/server.js",
    "test": "react-scripts test --env=jsdom",
    "test:coverage": "npm run test -- --coverage --coverageDirectory=jest-coverage --watchAll=false",
    "ci": "npm run test:coverage && run-p -r start cy:run:ci",
    "cy:open": "cypress open",
    "cy:run": "wait-on https://localhost:3000 && cypress run --headless --browser chrome",
    "cy:run:ci": "npm run cy:run -- --config-file cypress-test.json",
    "cy:verify": "npx cypress verify",
    "eslint": "eslint . --ext .js,.ts,.tsx",
    "eslint:fix": "npm run eslint -- --fix",
    "lint:styles": "stylelint \"src/**/*.{css,scss,sass}\" ",
    "lint:styles:report": "npm run lint:styles -- --formatter json --output-file ./styleLintReport.json",
    "report:merge": "mochawesome-merge ./cypress/results/mochawesome-report/*.json > tests-report.json",
    "report:generate": "marge tests-report.json -o cypress/reports/html",
    "report:copyScreenshots": "cp -r cypress/screenshots cypress/reports/html/screenshots",
    "snyk": "snyk test"
  },
  "browserslist": [
    ">0.2%",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@toast-ui/react-calendar": "1.0.5",
    "axios": "0.21.1",
    "bootstrap": "4.6.0",
    "chart.js": "2.9.4",
    "chartjs-adapter-date-fns": "2.0.0",
    "date-fns": "2.21.1",
    "eslint-plugin-cypress": "2.11.2",
    "express": "4.17.1",
    "font-awesome": "4.7.0",
    "ical": "0.8.0",
    "pg": "8.6.0",
    "query-string": "7.0.0",
    "react": "17.0.2",
    "react-beautiful-dnd": "13.1.0",
    "react-chartjs-2": "2.11.1",
    "react-dom": "17.0.2",
    "react-redux": "7.2.4",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.3",
    "reactstrap": "8.9.0",
    "rss-parser": "3.12.0",
    "winston": "3.3.3"
  },
  "devDependencies": {
    "@cypress/code-coverage": "3.9.5",
    "@cypress/instrument-cra": "1.4.0",
    "@types/chart.js": "2.9.32",
    "@types/enzyme": "3.10.8",
    "@types/enzyme-adapter-react-16": "1.0.6",
    "@types/express": "4.17.11",
    "@types/fb": "0.0.28",
    "@types/ical": "0.6.2",
    "@types/jest": "26.0.23",
    "@types/node": "14.14.41",
    "@types/pg": "7.14.11",
    "@types/react": "17.0.4",
    "@types/react-beautiful-dnd": "13.0.0",
    "@types/react-dom": "17.0.3",
    "@types/react-redux": "7.1.16",
    "@types/react-router-dom": "5.1.7",
    "@types/reactstrap": "8.7.2",
    "@types/xml2js": "0.4.8",
    "@typescript-eslint/eslint-plugin": "4.22.0",
    "@typescript-eslint/parser": "4.22.0",
    "cypress": "7.1.0",
    "cypress-multi-reporters": "1.5.0",
    "cypress-wait-until": "1.7.1",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.6",
    "eslint": "7.25.0",
    "eslint-config-prettier": "8.3.0",
    "istanbul-lib-coverage": "3.0.0",
    "mochawesome": "6.2.2",
    "mochawesome-merge": "4.2.0",
    "mochawesome-report-generator": "5.2.0",
    "node-sass": "5.0.0",
    "nyc": "15.1.0",
    "snyk": "1.566.0",
    "stylelint": "13.13.0",
    "stylelint-config-standard": "22.0.0",
    "ts-jest": "26.5.5",
    "tslib": "2.2.0",
    "tslint-react-hooks": "2.2.2",
    "typescript": "4.2.4",
    "wait-on": "5.3.0"
  }
}
