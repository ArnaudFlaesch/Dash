{
    "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier", "tslint-react-hooks"],
    "defaultSeverity": "warning",
    "rules": {
        "object-literal-sort-keys": false,
        "react-hooks-nesting": "error"
    },
    "linterOptions": {
        "exclude": [
            "src/**/I*.ts",
            "config/**/*.js",
            "node_modules/**/*.ts",
            "coverage/lcov-report/*.js"
        ]
    }
}